*** 25,30 ****
--- 25,33 ----
  .SUFFIXES: .po .mo .pot .ck
  .PHONY: all install uninstall prefixcheck converted check clean checkclean distclean update-po $(LANGUAGES)
  
+ LINGUAS:
+ 	@echo $(LANGUAGES) | tr " " "\n" |sed -e '/\./d' | sort > LINGUAS
+ 
  .po.mo:
  	$(MSGFMTCMD) -o $@ $<
  
